<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TimeTableNew" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorPrimaryDark</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->

<!--        transparent color below-->
        <item name="android:statusBarColor">@color/blue_bar</item>
<!--        <item name="android:statusBarColor">#D569C9F4</item>-->
<!--        <item name="android:statusBarColor">@android:color/transparent</item>-->
        <!-- Customize your theme here. -->
    </style>

    <color name="cardview_border">#2196F3</color>
    <color name="cardview_text_color">#888888</color>
    <color name="cardview_background">#ffffff</color>
    <color name="background_base">#ffffff</color>
    <color name="loginTextcolor">#888888</color>
    <color name="customDrawableColor">#ffffff</color>
    <color name="customTextColor">#000000</color>
    <color name="colorBackgroundBox">#ffffff</color>

    <style name="Theme.TimeTableNew.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

<!--    <style name="Theme.TimeTableNew.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

